{% for event in events %}
  {# <div class="grid-cell">{{event.humanDate()}}</div> #}
  <div class="grid-cell">{{event.seq}}</div>
  {% for chan in event.chans %}
    <div class="grid-cell">
      {% if chan is not none %}
        <a  class="button button-table {{ chan.css_class }}"
            href="{{ '/rubintv/' + camera.slug + '/' + chan.endpoint + '/' + event.cleanDate() + '/' + event.seq|string }}">
        </a>
      {% endif %}
    </div>
  {% endfor %}
{% endfor %}
